# SYNQ API credentials
# Obtain these from https://app.synq.io/settings/scout
SYNQ_CLIENT_ID=get-client-id-from-synq
SYNQ_CLIENT_SECRET=get-client-secret-from-synq

# OpenAI/LLM configuration
# API key for LiteLLM proxy (if required)
OPENAI_API_KEY=litellm-api-key

# Database connection credentials
# Add only the credentials you need for your specific database connections

# PostgreSQL
POSTGRES_PASSWORD=your-postgres-password

# MySQL
MYSQL_PASSWORD=your-mysql-password

# BigQuery
# Store the service account JSON as a single-line string
BIGQUERY_SERVICE_ACCOUNT_KEY=your-bigquery-service-account-key-json

# ClickHouse
CLICKHOUSE_PASSWORD=your-clickhouse-password

# Snowflake
# Note: Private key authentication is preferred (password auth is deprecated)
SNOWFLAKE_PRIVATE_KEY=your-snowflake-private-key-pem-content

# Redshift
REDSHIFT_PASSWORD=your-redshift-password

# Databricks
DATABRICKS_ACCESS_TOKEN=your-databricks-access-token

# Trino
# (Trino typically uses username-based auth without password in example)